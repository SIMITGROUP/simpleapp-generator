{
    "type": "object",
    "x-isolation-type":"tenant",  
    "x-page-type":"crud", 
    "required":["orgId","orgCode","orgName"],
    "properties": {
      "orgId": {
        "type": "integer",
        "default": 1,
        "minimum":1,
        "x-autocomplete-field":true        
      },
      "orgCode": {
        "type": "string",  
        "x-document-no":true,
        "minLength":1,      
        "examples": [
          "org1"
        ]
      },
      "orgName": {
        "type": "string",
        "x-document-label":true,
        "examples": [
          "Organization 1"
        ]
      },
      "active": {
        "type": "boolean",
        "default":true,
        "examples": [
          true
        ]
      },
      "description": {
        "type": "string",
        "format":"text",
        "examples": [
          "desc 1"
        ]
      },
      "timezone": {
        "type": "string",
        "examples": [
          "Asia/Kuala_Lumpur"
        ]
      }
      
    }
  }
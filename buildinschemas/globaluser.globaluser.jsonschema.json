{
  "type": "object",
  "x-isolation-type":"none",
  "required":["ssoId","email","isactive","fullname"],
  "properties": {
    "ssoId": {
      "type": "string",
      "x-document-no":true,
      "examples": [
        "xxxxx-yyyyy-zzzzz-aaa"
      ]
    },
    "email": {
      "type": "string",    
      "x-document-label":true,  
      "examples": [
        "myemail@abc.com"
      ],
      "format": "email"
    },
    "isactive": {
      "type": "boolean",
      "default":true,
      "examples": [
        true
      ]
    },
    "fullname": {
      "type": "string",
      "examples": [
        "user 1"
      ]
    },
    "username": {
      "type": "string",
      "examples": [
        "u1"
      ]
    },
    "gender": {
      "type": "string",
      "enum":["","M","F"],
      "examples": [
        "M"
      ]
    },
    "dob": {
      "type": "string",
      "format":"date",
      "examples": [
        "dateofbirth"
      ]
    },
    "tel": {
      "type": "string",
      "format":"tel",
      "examples": [
        "0197654321"
      ]
    }
  }
}
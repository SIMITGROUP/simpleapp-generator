export const getSubtotal = (details) =>{
    
    details.subtotal =  details.unitprice * details.quantity
    console.log(details)
    return details
}

export const getTotal = (details:any[]) => {
    if(details && details.length> 0){
        let total = 0
        details.forEach(item=>{
            total += item.subtotal
        })
        return total
    }
    return 0
}